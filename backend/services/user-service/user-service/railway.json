{
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "mvn clean package -DskipTests -q -Dmaven.test.skip=true",
    "watchPatterns": ["src/**", "pom.xml"]
  },
  "deploy": {
    "startCommand": "java -Xmx512m -Xms256m -XX:+UseG1GC -jar target/*.jar",
    "restartPolicyType": "ON_FAILURE"
  }
}